  rdfs:label "tipo de media"@es ;
  rdfs:label "tipo di media"@it ;
  rdfs:label "typ média"@cs ;
  rdfs:label "type de média"@fr ;
  rdfs:label "τύπος μέσου"@el ;
  rdfs:label "نوع الميديا"@ar ;
  rdfs:label "メディア・タイプ"@ja ;
